{
  "application": {
    "name": "Musical Loop Generator",
    "version": "1.0.0",
    "description": "Application for generating musical loops based on ambiance types using external APIs"
  },
  "api_keys": {
    "suno": "",
    "udio": "",
    "stable_audio": ""
  },
  "api_endpoints": {
    "suno": "https://api.suno.ai/v1/music",
    "udio": "https://api.udio.com/v1/generate",
    "stable_audio": "https://api.stableaudio.com/v1/sound"
  },
  "audio": {
    "sample_rate": 44100,
    "bit_depth": 16,
    "channels": 2,
    "format": "wav",
    "loop_duration": 30,
    "max_concurrent_requests": 3
  },
  "ambiance": {
    "default_type": "mysterious_forest",
    "config_directory": "../assets/ambiance_configs",
    "supported_types": [
      "mysterious_forest",
      "cyberpunk_rain",
      "sports_fans_chanting"
    ]
  },
  "ui": {
    "theme": "dark",
    "refresh_rate": 60,
    "visualizer_enabled": true
  },
  "logging": {
    "level": "INFO",
    "file": "logs/app.log",
    "max_size_mb": 10,
    "backup_count": 5
  },
  "cache": {
    "directory": "cache",
    "max_size_mb": 100,
    "ttl_seconds": 3600
  }
}